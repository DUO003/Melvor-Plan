script/source = "extends Node2D

var numbel=1#只支持0-9的单数字
var mode=0 #动画模式
var _png=\"\" #第一个字体
func _ready() -> void:
	
	var png=Sprite2D.new() #创建精灵
	png.texture=load(_png) #加载图片
	png.hframes=10 #水平设置10张
	png.frame=numbel #帧图设置
	#png.scale=Vector2(0.5,0.5) #设置缩放
	$\"数值\".add_child(png) #加入场景
	
	
	if mode==0:
		$AnimationPlayer.play(\"缩小放大\")
	elif mode==1:
		$AnimationPlayer.play(\"缩小放大上移动\")




func _on_animation_player_animation_finished(anim_name: StringName) -> void:
	
	queue_free()
	pass # Replace with function body.
"

